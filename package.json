{
  "name": "sports-tracker",
  "version": "1.0.0",
  "description": "App to follow and bet on sports",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server/server.js",
    "build": "babel-node server/server.js",
    "watch": "nodemon --exec npm run build",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run watch\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "daily-tasks": "node -e \"require('./server/utils/scheduledTasks').herokuTask()\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moviefan322/mlbapi.git"
  },
  "author": "Philip Neumann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moviefan322/mlbapi/issues"
  },
  "homepage": "https://github.com/moviefan322/mlbapi#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb-memory-server": "^8.12.2",
    "mongoose": "^7.1.0",
    "node-cron": "^3.0.2",
    "react-spinners": "^0.13.8",
    "socket.io": "^4.6.1",
    "spinners-react": "^1.0.7",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.5",
    "babel-node": "^0.0.1-security",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "react-testing-library": "^8.0.1",
    "supertest": "^6.3.3"
  }
}
